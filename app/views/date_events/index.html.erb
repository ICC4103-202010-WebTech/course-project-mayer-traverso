<p id="notice"><%= notice %></p>

<h1>Date Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_events.each do |date_event| %>
      <tr>
        <td><%= link_to 'Show', date_event %></td>
        <td><%= link_to 'Edit', edit_date_event_path(date_event) %></td>
        <td><%= link_to 'Destroy', date_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date Event', new_date_event_path %>
