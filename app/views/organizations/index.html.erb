<p id="notice"><%= notice %></p>

<h1>Organizations</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @organizations.each do |org| %>
    <tr>
      <td><%= org.name %></td>
      <td><%= link_to 'Show', organization_path(org.id) %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<br>
