<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
  <tr>
    <th>Event</th>
    <th>Date</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.name %></td>
      <td><%= event.date %></td>
      <td><%= link_to 'Show', event %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<br>
