<p id="notice"><%= notice %></p>

<h1>User Guests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_guests.each do |user_guest| %>
      <tr>
        <td><%= link_to 'Show', user_guest %></td>
        <td><%= link_to 'Edit', edit_user_guest_path(user_guest) %></td>
        <td><%= link_to 'Destroy', user_guest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Guest', new_user_guest_path %>
