<p id="notice"><%= notice %></p>

<%= link_to 'Edit', edit_message_path(@message) %> |
<%= link_to 'Back', messages_path %>

<h1>Messages</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @messages.each do |message| %>
    <tr>
      <td><%= message[0]%>:
      <br> <%=message[1]%>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>