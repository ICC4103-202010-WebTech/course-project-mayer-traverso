<p id="notice"><%= notice %></p>

<h1>Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |id| %>
      <tr>
        <td><%= link_to User.find(id).username, message_path(id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
